@model List<Student>
@{
    ViewData["Title"] = "Index";
}
<h1>List of Students</h1>
<hr />
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
            <th scope="col">Email</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Email</td>
            </tr>
        }
  
  
    </tbody>
</table>

